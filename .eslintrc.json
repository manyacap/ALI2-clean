{
  "env": {
    "browser": true,
    "es2021": true,
    "worker": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:import/recommended",
    "prettier"
  ],
  "plugins": ["import"],
  "rules": {
    "no-magic-numbers": ["error", { "ignore": [0, 1] }],
    "import/no-unresolved": ["error", { "ignore": ["^https://"] }]
  }
}
