{
  "name": "alicia-ia",
  "version": "7.3.0",
  "private": true,
  "type": "module",
  "scripts": {
    "prepare": "npx husky install",
    "dev": "npm exec vite",
    "typecheck": "tsc --noEmit",
    "build": "npm run typecheck && npm exec vite build",
    "preview": "npm exec vite preview --host 0.0.0.0 --port $PORT",
    "start": "npm run preview",
    "lint": "eslint \"src/**/*.{ts,js}\"",
    "format": "prettier --write \"src/**/*.{ts,js,css,html}\""
  },
  "dependencies": {
    "comlink": "^4.3.1",
    "workbox-routing": "^7.0.0",
    "workbox-precaching": "^7.0.0",
    "workbox-strategies": "^7.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.31.0",
    "@typescript-eslint/parser": "^8.31.0",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.2.6",
    "husky": "^8.0.0",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.4",
    "vite": "^4.5.13",
    "workbox-cli": "^6.5.4"
  },
  "engines": {
    "node": ">=14"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/manyacap/ALI2-clean.git"
  },
  "license": "MIT"
}

